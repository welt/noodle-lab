---
permalink: /feed.xml
eleventyExcludeFromCollections: true
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
  <channel>
    <title>Latest Release for {{ meta.repoName }}</title>
    <link>{{ meta.repoUrl }}</link>
    <description>Latest GitHub release tag for {{ meta.repoName }}</description>
    <language>en</language>
    {% if gitHubRelease and gitHubRelease.length %}
      {% for tag in gitHubRelease %}
        <item>
          <title>{{ tag.name }}</title>
          <link>{{ meta.repoUrl }}/tags/{{ tag.name }}</link>
          <description>Tag: {{ tag.name }}</description>
          <pubDate>{{ "now" | date("utc") }}</pubDate>
          <guid>{{ tag.commit.sha }}</guid>
        </item>
      {% endfor %}
    {% else %}
      <item>
        <title>Error fetching tag info</title>
        <description>Could not fetch the latest tag from GitHub. Please check the feed later.</description>
        <pubDate>{{ "now" | date("utc") }}</pubDate>
        <guid>error-{{ "now" | date("iso") }}</guid>
      </item>
    {% endif %}
  </channel>
</rss>
